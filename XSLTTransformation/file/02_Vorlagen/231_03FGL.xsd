<?xml version="1.0" encoding="ISO-8859-1"?><!-- edited with XMLSPY v2004 rel. 4 U (http://www.xmlspy.com) by tester (altova) --><xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema"><!-- FGL: Kopie von 223_01.xsd, aber für das Beispiel auf Seite 99ff. von mir verdoppelt -->  <xs:element name="Bis" type="xs:string"/>  <xs:element name="Tarifliste">    <xs:complexType>      <xs:sequence>        <xs:element name="Tarif" maxOccurs="unbounded">          <xs:complexType>            <xs:sequence>              <xs:element name="Name">                <xs:complexType>                  <xs:simpleContent>                    <xs:extension base="xs:string">                      <xs:attribute name="Nr" type="xs:byte" use="required"/>                    </xs:extension>                  </xs:simpleContent>                </xs:complexType>              </xs:element>              <xs:element name="Gueltigkeit">                <xs:complexType>                  <xs:sequence>                    <xs:element name="Datum">                      <xs:complexType>                        <xs:sequence>                          <xs:element ref="Von"/>                          <xs:element ref="Bis"/>                        </xs:sequence>                      </xs:complexType>                    </xs:element>                    <xs:element name="Uhrzeit">                      <xs:complexType>                        <xs:sequence>                          <xs:element ref="Von"/>                          <xs:element ref="Bis"/>                        </xs:sequence>                      </xs:complexType>                    </xs:element>                  </xs:sequence>                </xs:complexType>              </xs:element>              <xs:element name="Preis" type="xs:decimal"/>            </xs:sequence>          </xs:complexType>        </xs:element>      </xs:sequence>    </xs:complexType>  </xs:element>  <xs:element name="Von" type="xs:string"/></xs:schema>